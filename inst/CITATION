citHeader("Kindly cite the fmi R package as follows:")

citEntry(entry="misc",
         title = "fmi R package",
         author = personList(
		   person(given ="Joona", family="Lehtomaki", email = "joona.lehtomaki@gmail.com"),
                   person(given ="Jussi", family="Jousimo", email = "jvj@iki.fi"),		   
		   person(given ="Ilari", family="Scheinin"),
		   person(given ="Leo", family="Lahti", email = "leo.lahti@iki.fi")		   
         ),
	 journal = "",
	 year = "2014-2017",
         textVersion =
         paste("(C) Joona Lehtomaki, Jussi Jousimo, Ilari Scheinin, Leo Lahti / rOpenGov 2014-2017.",
               "fmi R package.\nURL: https://github.com/rOpenGov/fmi",
               sep=" ")
         )

citFooter("\nFor more info, see: https://github.com/rOpenGov/fmi")
